{
  "cleanupPeriodDays": 3650,
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(cat:*)",
      "Bash(ls:*)",
      "Bash(grep:*)",
      "Bash(sed:*)",
      "Bash(find:*)",
      "Bash(mkdir:*)",
      "Bash(cp:*)",
      "Bash(git:*)",
      "Bash(gh issue:*)",
      "Bash(gh pr:*)",
      "Bash(gh diff:*)",
      "Bash(gh release view:*)",
      "Bash(gh run list:*)",
      "Bash(gh run view:*)"
    ],
    "deny": [
      "Bash(sudo:*)",
      "Bash(rm:*)",
      "Bash(rm -fr:*)",
      "Bash(git clone:*)",
      "Bash(git rebase:*)",
      "Bash(git switch:*)",
      "Bash(git checkout:*)",
      "Bash(git add -A:*)",
      "Bash(git add --all:*)",
      "Bash(git commit -a:*)",
      "Bash(git commit --all:*)",
      "Bash(git push origin main:*)",
      "Bash(git push -f:*)",
      "Bash(git push --force:*)",
      "Bash(git push -u:*)",
      "Bash(git push --set-upstream:*)",
      "Bash(git branch -D:*)",
      "Bash(git worktree remove -f:*)",
      "Bash(git worktree remove --force:*)",
      "Bash(gh issue close:*)",
      "Bash(gh issue delete:*)",
      "Bash(gh pr merge:*)",
      "Bash(gh pr close:*)",
      "Bash(gh auth:*)",
      "Bash(gh gist:*)",
      "Bash(gh gpg-key:*)",
      "Bash(gh secret:*)",
      "Bash(gh ssh-key:*)",
      "Read(id_*)",
      "Read(credentials)",
      "Read(.credentials.json)",
      "Read(.env*)",
      "Read(**/secrets/**/)",
      "Read(**/*token*)",
      "Read(**/*key*)",
      "Bash(nc:*)",
      "Bash(wget:*)",
      "Bash(curl:*)",
      "Bash(sqlite3:*)",
      "Bash(psql:*)",
      "Bash(mysql:*)",
      "Bash(mongod:*)",
      "mcp__supabase__execute_sql"
